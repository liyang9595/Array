class Solution {
    public int removeElement(int[] nums, int val) {
        if(nums==null) {
	    		return 0;
	    	}
	    	int i=0;int j=0;
	    	while(i<nums.length) {
	    		if(nums[i]==val) {
	    			i++;
	    		}else {
	    			nums[j]=nums[i];
	    			j++;
	    			i++;
	    		}
	    	}
			return j;
	    	
	    }
}
