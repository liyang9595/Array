class Solution {
    public int removeDuplicates(int[] nums) {
	    	if(nums.length==1||nums==null) {
	    		return nums.length;
	    	}
		//双指针
	    	int i=0;
	    	int j=1;
	       while(j<nums.length) {
	        	if(nums[i]==nums[j]) {
	        		j++;
	        	}else {
	        		i++;
	        		nums[i]=nums[j];
	        		j++;
	        	}
	        }
	        
	        return i+1;
	    }
}
